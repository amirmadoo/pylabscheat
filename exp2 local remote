EXP12> ANSIBLE
sudo ansible --version
sudo su
cd /etc/ansible/
ls
rm -rf playbooks/
ls
mkdir playbooks
cd ..
cd home/apsit/Downloads
cat playbook.yml
name: nginx install and start services
hosts:all
become:true

tasks:
-name : installl nginx
apt:
name: nginx
state:latest

name: start nginx
service:
name:nginx
state:starter

